{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist"]
    },
    "build:ci": {
      "dependsOn": ["^build:ci"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".vercel/output/**", "dist"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "deploy": {
      "cache": false,
      "dependsOn": ["^deploy"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
